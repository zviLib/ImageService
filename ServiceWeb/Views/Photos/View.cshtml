@model IEnumerable<ServiceWeb.Models.Photo>
@{
    ViewBag.Title = "View";
}

<h2>View</h2>

<div class="row">
    <div>
            <img src=@ViewBag.PicPath alt=@ViewBag.Label>
            <div class="caption">
                <h3>@ViewBag.Label</h3>
                <p>@ViewBag.TakenMonth.@ViewBag.TakenYear</p>
                <a href="@Url.Action("Delete", "Photos", new { id= ViewBag.ID })" class="btn btn-default" role="button">Delete</a></p>
            </div>
    </div>
</div>

